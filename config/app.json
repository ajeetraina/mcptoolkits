{
  "refreshInterval": 30,
  "theme": "light",
  "alertThresholds": {
    "latency": {
      "warning": 30,
      "critical": 50
    },
    "uptime": {
      "warning": 99.5,
      "critical": 99.0
    },
    "memory": {
      "warning": 85,
      "critical": 95
    },
    "cpu": {
      "warning": 75,
      "critical": 90
    },
    "disk": {
      "warning": 80,
      "critical": 90
    }
  },
  "defaultView": "overview",
  "features": {
    "enableNotifications": true,
    "enableHistoricalData": true,
    "enableExport": true,
    "enableAutoRefresh": true,
    "enableDarkMode": false
  },
  "charts": {
    "performance": {
      "timeRange": "24h",
      "dataPoints": 12,
      "metrics": ["latency", "cpu", "memory"]
    },
    "userActivity": {
      "timeRange": "24h",
      "dataPoints": 12
    }
  },
  "demo": {
    "enabled": true,
    "sampleData": true
  }
}